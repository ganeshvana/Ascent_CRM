<?xml version="1.0"?>
<odoo>
<!--    Contracting-->
        <record id="crm_lead_contract_entity_form" model="ir.ui.view">
            <field name="name">crm.lead.contract.entity.form</field>
            <field name="model">crm.lead.contract.entity</field>
            <field name="arch" type="xml">
                <form string="Ascent Contract Entity">
                    <sheet>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="name"/>
                            </div>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="more_details"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="crm_lead_contract_entity_tree" model="ir.ui.view">
            <field name="name">crm.lead.contract.entity.tree</field>
            <field name="model">crm.lead.contract.entity</field>
            <field name="arch" type="xml">
                <tree string="Ascent Contract Entity" editable="bottom">
                    <field name="name"/>
                    <field name="more_details"/>
                </tree>
            </field>
        </record>

        <record id="crm_lead_contract_entity_action" model="ir.actions.act_window">
            <field name="name">Ascent Contract Entity</field>
            <field name="res_model">crm.lead.contract.entity</field>
            <field name="view_id" ref="crm_lead_contract_entity_tree"/>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create new tags contract entity
              </p>
            </field>
        </record>

        <menuitem
            id="menu_contract_entity_categ"
            name="Ascent Contract Entity"
            action="crm_lead_contract_entity_action"
            parent="crm.menu_crm_config_lead"
            sequence="200"/>


